{
    "entryPoints": [
        "./src/*"
    ],
    "plugin": [
        "typedoc-plugin-markdown"
    ],
    "githubPages": false,
    "readme": "none",
    "indexFormat": "table",
    "useCodeBlocks": true,
    "disableSources": true,
    "excludeScopesInPaths": true,
    "textContentMappings": {
        "title.indexPage": "API Reference",
        "title.memberPage": "{name}"
    },
    "out": "./docs"
}